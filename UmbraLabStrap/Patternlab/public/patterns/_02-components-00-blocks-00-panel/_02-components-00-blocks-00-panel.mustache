<div class="panel {{panelClass}}">
  {{# panelHeading }}
  <div class="panel-heading">
    <h3 class="panel-title">{{ panelHeading }}</h3>
  </div>
  {{/ panelHeading }}
  <div class="panel-body">
    {{ text }}
  </div>
  {{# panelFooter }}
  <div class="panel-footer">{{ panelFooter}}</div>
  {{/ panelFooter }}
</div>
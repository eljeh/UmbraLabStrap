<!-- Panels for push down navigation from main nav -->
{{>organisms-panel-find-your-home}}
{{>organisms-panel-investors}}
{{>organisms-panel-search}}
{{>organisms-mobile-navigation}}

<div class="wrapper">

    {{> organisms-header}}

    {{> molecules-jumbotron:vertical-center|jumbotron-medium}}

    <div class="container-fluid bg-darkblue">
        <div class="row">
            <div class="col-md-12">
                {{>organisms-icon-navigation:icon-navigation|d-flex|flex-row|justify-content-between|align-items-start|flex-wrap|white}}
            </div>
        </div> <!--/.row-->
    </div>

    <main>

        <section class="multi-columns">
            <div class="container">

                <div class="column-wrapper">

                <div class="excerpt-card">
                    <div class="excerpt-card-text">

                    <h4 class="excerpt-card-subtitle">2017 Boardwalk REIT Investor Presentations</h4>

                        <ul class="icon-file-list d-flex flex-row justify-content-between align-items-start flex-wrap">
                            <li>
                                {{>atoms-icon-link(icon-link-url:"#", icon-link-icon-class:"icon-financials-and-releases", icon-link-text:"Q4 2017 Boardwalk REIT Investor Presentations")}}
                            </li>
                            <li>
                                {{>atoms-icon-link(icon-link-url:"#", icon-link-icon-class:"icon-presentations", icon-link-text:"Q3 2017 Boardwalk REIT Investor Presentations")}}
                            </li>
                            <li>
                                {{>atoms-icon-link(icon-link-url:"#", icon-link-icon-class:"icon-presentations", icon-link-text:"Q2 2017 Boardwalk REIT Investor Presentations")}}
                            </li>
                            <li>
                                {{>atoms-icon-link(icon-link-url:"#", icon-link-icon-class:"icon-presentations", icon-link-text:"Q1 2017 Boardwalk REIT Investor Presentations")}}
                            </li>

                        </ul> 

                    </div>
                </div>
            </div>
        </section>

    </main>

    {{> organisms-footer}}

</div> <!--/.wrapper-->